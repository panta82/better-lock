{
  "name": "better-lock",
  "version": "2.0.0-beta",
  "description": "A (better) node.js lock library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "test": "jest",
    "watch": "jest --watchAll",
    "lint": "eslint ./src",
    "coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panta82/better-lock.git"
  },
  "keywords": [
    "lock",
    "mutex",
    "async",
    "wait",
    "enter",
    "block",
    "sync"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^27.0.2",
    "prettier": "latest",
    "prettier-plugin-import-sort": "0.0.7",
    "ts-jest": "^27.0.1",
    "typescript": "^4.3.2"
  },
  "author": "Ivan Pantic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/panta82/better-lock/issues"
  },
  "homepage": "https://github.com/panta82/better-lock#readme",
  "importSort": {
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    },
    ".js, .jsx": {
      "style": "module"
    }
  }
}
